        <attr name="bottomPositiveButtonText" format="string" />
        <attr name="bottomPositiveButtonBackground" format="color" />

        <attr name="bottomNegativeButtonText" format="string" />
        <attr name="bottomNegativeButtonBackground" format="color" />

        <attr name="bottomProgressMode" format="boolean" />
    </declare-styleable>

    <attr name="bottomButtonsStyle" format="reference" />

    <declare-styleable name="TicTacToeView">
